[core]
        editor = vim
        excludesfile = ~/.gitignore
[push]
        default = matching
[filter "media"]
        required = true
        clean = git media clean %f
        smudge = git media smudge %f
[filter "hawser"]
        clean = git hawser clean %f
        smudge = git hawser smudge %f
        required = true
[filter "lfs"]
        clean = git lfs clean %f
        smudge = git lfs smudge %f
        required = true
[alias]
        co = checkout
        br = branch
        ci = commit
        st = status
        last = "log -1 HEAD"
        unstage = "reset HEAD --"
        hist = "log --decorate --graph --oneline --all"
        pull = pull --rebase
